<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <!--
    <parameters>
        <parameter key="rtx_labs_user.example.class">RtxLabs\UserBundle\Example</parameter>
    </parameters>
    -->

    <services>
        <service id="rtxlabs.user.user_manager" class="RtxLabs\UserBundle\Entity\User">
            <argument type="service" id="security.encoder_factory" />
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument>Rotex\Sbp\CoreBundle\Entity\User</argument>
        </service>
    </services>

    <services>
        <service id="rtxlabs.user.login_success" class="Rotex\Sbp\CoreBundle\Handler\LoginHandler">
            <argument type="service" id="doctrine.orm.entity_manager" />
        </service>
    </services>

</container>
